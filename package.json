{
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "version": "0.0.27",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "build": "webpack && rollup -c",
    "build:dev": "webpack --mode=development && rollup -c -w",
    "check": "npm run build && npm run test && npm run coverage && npm run lint",
    "pretest": "npm run build",
    "docs:generate": "cat support/README.md > README.md && jsdoc2md dist/index.esm.js >> README.md",
    "docs:commit": "git add README.md support && git diff-index --quiet HEAD || git commit -m \"update documentation\"",
    "docs": "npm run docs:generate && npm run docs:commit",
    "prepublishOnly": "npm run build && npm run docs"
  },
  "files": [
    "dist"
  ],
  "name": "react-process-env",
  "description": "express utility to safely inject variables from process.env (node.js) into window.env (react)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/specialblend/react-process-env.git"
  },
  "keywords": [
    "react",
    "process",
    "env"
  ],
  "author": "cj@specialblend.net",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/specialblend/react-process-env/issues"
  },
  "homepage": "https://github.com/specialblend/react-process-env#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "@specialblend/eslint-config": "0.0.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-plugin-babel": "^5.3.0",
    "jest": "^24.8.0",
    "jest-extended": "^0.11.1",
    "jsdoc-to-markdown": "^5.0.0",
    "rollup": "^1.12.0",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "ramda": "^0.26.1"
  }
}
